void Holder::setFio(std::string fio)
{
    m_fio = fio;
}


bool Land::addHolder(Holder &holder, Fraction part)
{
    if(sumParts() + part.value() > 1)
    {
        return false;
    }
    m_holders.push_back(holder);
    m_parts.push_back(part);
    return true;
}
